<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/eventListener.xsd">
	<import>
		<eventlistener name="gmailGuardRegistrationValidate">
			<eventclassname>wcf\form\RegisterForm</eventclassname>
			<eventname>validate</eventname>
			<listenerclassname>wcf\system\event\listener\GmailGuardRegistrationListener</listenerclassname>
			<inherit>0</inherit>
		</eventlistener>

		<eventlistener name="gmailGuardRegistrationSaved">
			<eventclassname>wcf\form\RegisterForm</eventclassname>
			<eventname>saved</eventname>
			<listenerclassname>wcf\system\event\listener\GmailGuardRegistrationListener</listenerclassname>
			<inherit>0</inherit>
		</eventlistener>
	</import>
</data>
